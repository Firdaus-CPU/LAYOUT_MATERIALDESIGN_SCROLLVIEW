<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="30dp"
    tools:context=".MainActivity"
    tools:ignore="ExtraText">
    <RelativeLayout
        android:id="@+id/Garis1"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/Gambar1"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginTop="10dp"
            app:srcCompat="@drawable/gambar1" />

        <Button
            android:id="@+id/Button1"
            style="@style/Widget.MaterialComponents.Button.TextButton"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_below="@+id/Gambar1"
            android:text="Facebook"
            android:textStyle="bold"
            android:textSize="22px" />

        <ImageView
            android:id="@+id/Gambar2"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@+id/Gambar1"
            app:srcCompat="@drawable/gambar2" />

        <Button
            android:id="@+id/Button2"
            style="@style/Widget.MaterialComponents.Button.TextButton"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/Button1"
            android:layout_below="@+id/Gambar2"
            android:text="Pinterest"
            android:textStyle="bold"
            android:textSize="22px" />

        <ImageView
            android:id="@+id/Gambar3"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@+id/Gambar2"
            app:srcCompat="@drawable/gambar3" />

        <Button
            android:id="@+id/Button3"
            style="@style/Widget.MaterialComponents.Button.TextButton"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/Button2"
            android:layout_below="@+id/Gambar3"
            android:text="Whatsapp"
            android:textStyle="bold"
            android:textSize="22px" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/Garis2"
        android:layout_width="match_parent"
        android:layout_marginTop="40dp"
        android:layout_below="@id/Garis1"
        android:layout_height="120dp"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/Gambar4"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginTop="10dp"
            app:srcCompat="@drawable/gambar4" />

        <Button
            android:id="@+id/Button4"
            style="@style/Widget.MaterialComponents.Button.TextButton"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_below="@+id/Gambar4"
            android:text="map"
            android:textStyle="bold"
            android:textSize="22px"  />
        <ImageView
            android:id="@+id/Gambar5"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/Gambar4"
            app:srcCompat="@drawable/gambar5" />

        <Button
            android:id="@+id/Button5"
            style="@style/Widget.MaterialComponents.Button.TextButton"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/Button4"
            android:layout_below="@+id/Gambar5"
            android:text="vk"
            android:textStyle="bold"
            android:textSize="22px" />
        <ImageView
            android:id="@+id/Gambar6"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/Gambar5"
            app:srcCompat="@drawable/gambar6" />

        <Button
            android:id="@+id/Button6"
            style="@style/Widget.MaterialComponents.Button.TextButton"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/Button5"
            android:layout_below="@+id/Gambar6"
            android:text="instagram"
            android:textStyle="bold"
            android:textSize="20px"  />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/Garis3"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_below="@id/Garis2"
        android:layout_marginTop="40dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/Gambar7"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginTop="10dp"
            app:srcCompat="@drawable/gambar7" />

        <Button
            android:id="@+id/Button7"
            style="@style/Widget.MaterialComponents.Button.TextButton"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_below="@+id/Gambar7"
            android:text="spotify"
            android:textSize="22px"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/Gambar8"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@+id/Gambar7"
            app:srcCompat="@drawable/gambar9" />

        <Button
            android:id="@+id/Button8"
            style="@style/Widget.MaterialComponents.Button.TextButton"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_below="@+id/Gambar8"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="40dp"
            android:layout_marginBottom="30dp"
            android:layout_toRightOf="@+id/Button7"
            android:text="SOund Cloud"
            android:textSize="22px"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/Gambar9"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@+id/Gambar8"
            app:srcCompat="@drawable/gambar8" />

        <Button
            android:id="@+id/Button9"
            style="@style/Widget.MaterialComponents.Button.TextButton"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_below="@+id/Gambar9"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/Button8"
            android:text="Skype"
            android:textStyle="bold"
            android:textSize="22px"  />
    </RelativeLayout>


</RelativeLayout>